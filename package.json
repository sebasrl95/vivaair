{
  "name": "vivaair",
  "description": "Bot de testeo para Viva Air",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-unit && npm run test-integration-runner",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "serve": "nodemon server.js",
    "codecov": "npm run test && (codecov || true)"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.1.1",
    "buffer": "^5.0.7",
    "cors": "^2.8.4",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "fs": "0.0.1-security",
    "jquery": "^3.2.1",
    "jsonfile": "^3.0.1",
    "lightgallery": "^1.6.11",
    "nodemailer": "^4.1.1",
    "npm": "^6.1.0",
    "registry-url": "^3.1.0",
    "server": "^1.0.0-beta.1",
    "soap": "^0.23.0",
    "watson-developer-cloud": "^2.37.0",
    "xhr-request": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "casperjs": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^2.8.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.17.4",
    "phantomjs-prebuilt": "^2.1.13",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": ">= 6.9.x",
    "npm": "> 3.10.x"
  }
}
